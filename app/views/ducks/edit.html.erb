<h1> Edit: <%= @duck.name %> </h1>
<% if flash[:duck_errors] %>
    <%flash[:duck_errors].each do |error| %>
         <p style="color:red;"><%= error %></p>
    <% end %>
<% end %>

<%= form_for(@duck) do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :description %>
    <%= f.text_field :description %>

    <%= f.label :student_id %>
    <%= f.collection_select :student_id, Student.all, :id, :name%>

    <%= f.submit 'Edit New Duck' %>
        
<% end %>